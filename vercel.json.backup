{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "redirects": [
    { "source": "/home/f/:slug*", "destination": "/blog/:slug*", "permanent": true },
    { "source": "/f/:slug*", "destination": "/blog/:slug*", "permanent": true },
    { "source": "/blog/:slug*", "has": [{ "type": "query", "key": "blogcategory" }], "destination": "/blog/:slug*", "permanent": true },
    { "source": "/f/:slug*", "has": [{ "type": "query", "key": "blogcategory" }], "destination": "/blog/:slug*", "permanent": true },
    { "source": "/home/f/:slug*", "has": [{ "type": "query", "key": "blogcategory" }], "destination": "/blog/:slug*", "permanent": true },

    { "source": "/pages/contact", "destination": "/about", "permanent": true },
    { "source": "/services", "destination": "/", "permanent": true },
    { "source": "/collections/:path*", "destination": "/", "permanent": true },
    { "source": "/products/:path*", "destination": "/", "permanent": true },
    { "source": "/password", "destination": "/", "permanent": true },
    { "source": "/privacy-policy", "destination": "/privacy", "permanent": true },
    { "source": "/terms-and-conditions", "destination": "/terms", "permanent": true },
    { "source": "/medicare-billing", "destination": "/medicare", "permanent": true },
    { "source": "/home", "destination": "/", "permanent": true },
    { "source": "/testimonials", "destination": "/about", "permanent": true },
    { "source": "/book-here", "destination": "/tools", "permanent": true },
    { "source": "/our-team", "destination": "/about", "permanent": true },
    { "source": "/questions", "destination": "/faq", "permanent": true },
    { "source": "/downloadable-resources", "destination": "/tools", "permanent": true },
    { "source": "/receptionist", "destination": "/about", "permanent": true },
    { "source": "/subscribe-to-365-care", "destination": "/pricing", "permanent": true },
    { "source": "/weight-loss-clinics", "destination": "/", "permanent": true },
    { "source": "/weight-loss-clinic", "destination": "/", "permanent": true },
    { "source": "/online-consultation", "destination": "/how-it-works", "permanent": true },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "downscale.com.au" }],
      "destination": "https://www.downscale.com.au/:path*",
      "permanent": true
    }
  ],
