    "lighthouse": "lighthouse https://downscale.com.au --output html --output-path ./lighthouse-report.html",
    "sitemap:build": "node ./scripts/crawl-generate-sitemap.mjs",
    "lhci:collect": "lhci collect --url=https://www.downscale.com.au/ --numberOfRuns=3",
    "lhci:assert": "lhci assert --config=./lighthouserc.cjs",
    "lhci": "npm run lhci:collect && npm run lhci:assert",
    "seo:pull": "node ./scripts/gsc-export.mjs",
    "benchmark": "node ./scripts/benchmark.mjs",
