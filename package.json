{
  "name": "downscale-nextjs",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run generate:sitemaps",
    "build": "next build",
    "build:dev": "next build",
    "build:analyze": "next build && npx @next/bundle-analyzer",
    "lint": "next lint",
    "seo:check": "node scripts/seo-check.cjs",
